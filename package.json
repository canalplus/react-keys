{
  "name": "react-keys",
  "version": "1.14.2",
  "private": false,
  "description": "Simple way to bind keyboard to react thinking with flux",
  "keywords": [
    "react",
    "keys",
    "keyboard",
    "flux"
  ],
  "main": "build",
  "author": "Canal+",
  "homepage": "https://github.com/canalplus/react-keys",
  "bugs": "https://github.com/canalplus/react-keys/issues",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/canalplus/react-keys.git"
  },
  "scripts": {
    "prepublish": "npm run clean && npm run build && npm run dist",
    "clean": "rimraf build && rimraf dist",
    "preversion": "npm run test",
    "postversion": "git push && git push --tags",
    "version": "npm run build",
    "build": "babel src/ -d build",
    "lint": "eslint src/** test/**",
    "dist": "npm run test && webpack --progress --colors --optimize-minimize",
    "test": "npm run lint && mocha --compilers js:babel-core/register --require test/dom-config.js test/config.js test/*.spec.js test/**/*.spec.js --recursive",
    "coverage": "babel-node ./node_modules/istanbul/lib/cli.js cover _mocha -- test/dom-config.js test/config.js test/*.spec.js test/**/*.spec.js"
  },
  "devDependencies": {
    "babel-cli": "6.14.0",
    "babel-core": "6.14.0",
    "babel-eslint": "6.1.2",
    "babel-loader": "6.2.5",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-1": "6.13.0",
    "chai": "3.5.0",
    "chai-enzyme": "0.5.1",
    "codecov": "1.0.1",
    "enzyme": "2.4.1",
    "eslint": "3.4.0",
    "eslint-plugin-import": "1.14.0",
    "eslint-plugin-jsx-a11y": "2.2.0",
    "eslint-plugin-react": "6.1.2",
    "express": "4.14.0",
    "istanbul": "1.0.0-alpha.2",
    "jsdom": "9.4.2",
    "mocha": "3.0.2",
    "react": "15.3.1",
    "react-dom": "15.3.1",
    "react-redux": "4.4.5",
    "redux": "3.5.2",
    "rimraf": "2.5.4",
    "sinon": "1.17.5",
    "sinon-chai": "2.8.0",
    "webpack": "1.13.2",
    "webpack-dev-middleware": "1.6.1",
    "webpack-livereload-plugin": "0.8.2",
    "winston": "^2.2.0"
  },
  "peerDependencies": {
    "react": "0.14.x || 15.x.x",
    "react-dom": "0.14.x || 15.x.x"
  }
}
